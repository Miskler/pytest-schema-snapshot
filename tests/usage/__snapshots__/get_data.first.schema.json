{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "обязательная": {
      "type": "string",
      "examples": [
        "строка"
      ]
    },
    "необязательная": {
      "type": "null",
      "examples": [
        null
      ]
    },
    "словарь": {
      "type": "object",
      "properties": {
        "ключ": {
          "type": "string",
          "examples": [
            "значение"
          ]
        },
        "число": {
          "type": "integer",
          "examples": [
            123
          ]
        }
      },
      "required": [
        "ключ",
        "число"
      ],
      "examples": [
        {
          "ключ": "значение",
          "число": 123
        }
      ]
    }
  },
  "required": [
    "необязательная",
    "обязательная",
    "словарь"
  ],
  "examples": [
    {
      "обязательная": "строка",
      "необязательная": null,
      "словарь": {
        "ключ": "значение",
        "число": 123
      }
    }
  ]
}