{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "examples": [
        "John Doe"
      ]
    },
    "description": {
      "type": "string",
      "examples": [
        "This is a regular text string"
      ]
    },
    "status": {
      "type": "string",
      "examples": [
        "active"
      ]
    }
  },
  "required": [
    "description",
    "name",
    "status"
  ],
  "examples": [
    {
      "name": "John Doe",
      "description": "This is a regular text string",
      "status": "active"
    }
  ]
}